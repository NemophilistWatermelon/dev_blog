$profileColor = #888
.profileBtn-group
    margin-top 20px
    margin-bottom 20px
 .profileBtn-group .blogPro-icon
    display: inline-block;
    width 40px
    height: 40px
    margin 0 10px
    font-size 18px
    line-height: 40px
    color #fff
    text-decoration: none;
    border-radius: 50%
    background-color:$profileColor;
    filter: blur(3px)
    transition all .5s
    overflow: hidden;
 .profileBtn-group .blogPro-icon:hover
    border-radius:3px
    color: #ff
    filter: blur(0)
